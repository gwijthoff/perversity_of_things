	keywords = {\_tablet, Batteries, Business And Economics, Business models, Entrepreneurs, History, Industrial research, Innovations, Inventors, Patents, Studies},
	pages = {1--13},
	file = {Hintz_2007_Independent_Inventors_in_an_Era_of_Burgeoning_Research_&_Development.pdf:/Users/grantwythoff/Dropbox/zotero/Hintz_2007_Independent_Inventors_in_an_Era_of_Burgeoning_Research_&_Development.pdf:application/pdf;Snapshot:/Users/grantwythoff/Library/Application Support/Firefox/Profiles/lcvvi5mw.default/zotero/storage/TA93KGG4/192404483.html:text/html}
